<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textColor="#CCCCCC"
    android:background="@color/white"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/search_bar"
        android:layout_width="match_parent"
        android:layout_height="60sp"
        android:paddingHorizontal="30sp"
        android:paddingVertical="5sp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingHorizontal="10sp"
            android:background="#F9F9F9">

            <ImageView
                android:id="@+id/ic_search"
                android:layout_width="35sp"
                android:layout_height="35sp"
                android:src="@drawable/ic_search_grayo"
                android:layout_centerVertical="true"/>

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginStart="10sp"
                android:layout_marginTop="10sp"
                android:layout_marginEnd="10sp"
                android:layout_marginBottom="10sp"
                android:layout_toEndOf="@id/ic_search"
                android:background="@null"
                android:text="Search"
                android:textColor="#CCCCCC"
                android:textSize="20sp" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/fragment_body"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/search_bar">

        <RelativeLayout
            android:id="@+id/top1"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:paddingHorizontal="30sp"
            android:paddingVertical="5sp">

            <TextView
                style="@style/size14Text_DavysGray"
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="80sp"
                android:layout_marginTop="20sp"
                android:layout_marginEnd="10sp"
                android:layout_marginBottom="10sp"
                android:text="Name" />

            <TextView
                style="@style/size14Text_DavysGray"
                android:layout_width="50dp"
                android:layout_height="20dp"
                android:layout_marginStart="190sp"
                android:layout_marginTop="20sp"
                android:layout_marginEnd="10sp"
                android:layout_marginBottom="10sp"
                android:text="Qty" />

            <TextView
                android:layout_width="50dp"
                android:layout_height="20dp"
                android:layout_marginStart="280sp"
                android:layout_marginTop="20sp"
                android:layout_marginEnd="10sp"
                android:layout_marginBottom="10sp"
                android:text="Expiry"
                style="@style/size14Text_DavysGray"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/item_top1"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_below="@id/top1"
            android:paddingHorizontal="30sp"
            android:paddingVertical="5sp">

            <View
                android:layout_width="500sp"
                android:layout_marginStart="0sp"
                android:layout_height="2sp"
                android:background="#c0c0c0" />

            <ImageView
                android:id="@+id/item_pic1"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_marginTop="10sp"
                android:src="@drawable/calcidinplus" />

            <TextView
                android:id="@+id/item_name1"
                style="@style/size12Text_DavysGray"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="60sp"
                android:layout_marginTop="10sp"
                android:gravity="center"
                android:text="Calcidin Plus" />

            <TextView
                android:id="@+id/item_qty1"
                style="@style/size12Text_DavysGray"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="160sp"
                android:layout_marginTop="10sp"
                android:gravity="center"
                android:text="0" />

            <TextView
                android:id="@+id/item_expiry1"
                style="@style/size12Text_DavysGray"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="260sp"
                android:layout_marginTop="10sp"
                android:gravity="center"
                android:text="n/a" />

        </RelativeLayout>



        <RelativeLayout
            android:id="@+id/item_top2"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_below="@id/item_top1"
            android:paddingHorizontal="30sp"
            android:paddingVertical="5sp">

            <View
                android:layout_width="500sp"
                android:layout_marginStart="0sp"
                android:layout_height="2sp"
                android:background="#c0c0c0" />

            <ImageView
                android:id="@+id/item_pic2"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_marginTop="10sp"
                android:src="@drawable/heragest"
                />

            <TextView
                android:id="@+id/item_name2"
                style="@style/size12Text_DavysGray"
                android:gravity="center"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="60sp"
                android:layout_marginTop="10sp"
                android:text="Heragest" />

            <TextView
                android:id="@+id/item_qty2"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="160sp"
                android:layout_marginTop="10sp"
                android:text="0"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

            <TextView
                android:id="@+id/item_expiry2"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="260sp"
                android:layout_marginTop="10sp"
                android:text="n/a"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

        </RelativeLayout>



        <RelativeLayout
            android:id="@+id/item_top3"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_below="@id/item_top2"
            android:paddingHorizontal="30sp"
            android:paddingVertical="5sp">

            <View
                android:layout_width="500sp"
                android:layout_marginStart="0sp"
                android:layout_height="2sp"
                android:background="#c0c0c0" />

            <ImageView
                android:id="@+id/item_pic3"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_marginTop="10sp"
                android:src="@drawable/quatrofol"
                />

            <TextView
                android:id="@+id/item_name3"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="60sp"
                android:layout_marginTop="10sp"
                android:text="Quatrofol"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

            <TextView
                android:id="@+id/item_qty3"
                style="@style/size12Text_DavysGray"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="160sp"
                android:layout_marginTop="10sp"
                android:text="0"
                android:gravity="center"/>

            <TextView
                android:id="@+id/item_expiry3"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="260sp"
                android:layout_marginTop="10sp"
                android:text="n/a"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/item_top4"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_below="@id/item_top3"
            android:paddingHorizontal="30sp"
            android:paddingVertical="5sp">

            <View
                android:layout_width="500sp"
                android:layout_marginStart="0sp"
                android:layout_height="2sp"
                android:background="#c0c0c0" />

            <ImageView
                android:id="@+id/item_pic4"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_marginTop="10sp"
                android:src="@drawable/tempraforte"
                />

            <TextView
                android:id="@+id/item_name4"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="60sp"
                android:layout_marginTop="10sp"
                android:text="TempraForte"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

            <TextView
                android:id="@+id/item_qty4"
                style="@style/size12Text_DavysGray"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="160sp"
                android:layout_marginTop="10sp"
                android:text="0"
                android:gravity="center"/>

            <TextView
                android:id="@+id/item_expiry4"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="260sp"
                android:layout_marginTop="10sp"
                android:text="n/a"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/item_top5"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_below="@id/item_top4"
            android:paddingHorizontal="30sp"
            android:paddingVertical="5sp">

            <View
                android:layout_width="500sp"
                android:layout_marginStart="0sp"
                android:layout_height="2sp"
                android:background="#c0c0c0" />

            <ImageView
                android:id="@+id/item_pic5"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_marginTop="10sp"
                android:src="@drawable/vaxigriptetra" />

            <TextView
                android:id="@+id/item_name5"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="60sp"
                android:layout_marginTop="10sp"
                android:text="VaxigripTetra"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

            <TextView
                android:id="@+id/item_qty5"
                style="@style/size12Text_DavysGray"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="160sp"
                android:layout_marginTop="10sp"
                android:text="0"
                android:gravity="center"/>

            <TextView
                android:id="@+id/item_expiry5"
                android:layout_width="80sp"
                android:layout_height="40sp"
                android:layout_marginStart="260sp"
                android:layout_marginTop="10sp"
                android:text="n/a"
                android:gravity="center"
                style="@style/size12Text_DavysGray"/>

        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>